<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="MaterialSpinner" tools:ignore="ResourceName">
        <attr name="px_arrow_tint" format="color" />
        <attr name="px_hide_arrow" format="boolean" />
        <attr name="px_background_color" format="color" />
        <attr name="px_text_color" format="color" />
        <attr name="px_icon_size" format="dimension" />
        <attr name="px_spinner_max_height" format="dimension" />
        <attr name="px_spinner_height" format="dimension">
            <enum name="fill_parent" value="-1" />
            <enum name="match_parent" value="-1" />
            <enum name="wrap_content" value="-2" />
        </attr>
        <attr name="px_background_selector" format="integer" />
        <attr name="px_padding_top" format="dimension" />
        <attr name="px_padding_left" format="dimension" />
        <attr name="px_padding_bottom" format="dimension" />
        <attr name="px_padding_right" format="dimension" />
        <attr name="px_popup_padding_top" format="dimension" />
        <attr name="px_popup_padding_left" format="dimension" />
        <attr name="px_popup_padding_bottom" format="dimension" />
        <attr name="px_popup_padding_right" format="dimension" />
        <attr name="px_hint" format="string" />
        <attr name="px_hint_color" format="color" />
        <attr name="px_placeholder" format="reference" />
    </declare-styleable>
</resources>